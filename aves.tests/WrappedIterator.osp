use aves;

namespace aves.tests;

// Implements an iterator that wraps an existing Iterator and does not
// override any non-abstract methods. This is for testing aves.Iterator.
private class WrappedIterator is Iterator
{
	public new(this.inner);

	private inner;

	override get current = inner.current;

	override moveNext()
	{
		return inner.moveNext();
	}
}
